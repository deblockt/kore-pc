<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.TilePane?>

<BorderPane styleClass="background" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.view.PlayingController">
   <top>
      <ToolBar prefHeight="40.0" prefWidth="200.0" styleClass="background-menu" BorderPane.alignment="CENTER">
         <items>
            <Label fx:id="backButton" styleClass="menu-action" text="Retour" />
         </items>
      </ToolBar>
   </top>
   <center>
      <TabPane fx:id="tabPane" prefHeight="200.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
