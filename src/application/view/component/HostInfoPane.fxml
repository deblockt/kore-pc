<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.TilePane?>

<fx:root type="javafx.scene.layout.GridPane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.40">

  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
  </rowConstraints>
   <children>
      <Label text="Nom" />
      <TextField fx:id="name" GridPane.columnIndex="1" />
      <Label text="Ip" GridPane.rowIndex="1" />
      <TextField fx:id="host" GridPane.columnIndex="1" GridPane.rowIndex="1" />
      <Label text="Protocol" GridPane.rowIndex="2" />
      <ComboBox fx:id="protocol" prefWidth="150.0" GridPane.columnIndex="1" GridPane.rowIndex="2" />
      <Label text="HTTP port" GridPane.rowIndex="3" />
      <Label text="TCP port" GridPane.rowIndex="4" />
      <Label text="Nom d'utilisateur" GridPane.rowIndex="5" />
      <Label text="Mot de passe" GridPane.rowIndex="6" />
      <Label text="Evenement port" GridPane.rowIndex="7" />
      <Spinner fx:id="httpPort" editable="true" GridPane.columnIndex="1" GridPane.rowIndex="3" />
      <Spinner fx:id="tcpPort" editable="true" GridPane.columnIndex="1" GridPane.rowIndex="4" />
      <Spinner fx:id="eventPort" editable="true" GridPane.columnIndex="1" GridPane.rowIndex="7" />
      <TextField fx:id="username" GridPane.columnIndex="1" GridPane.rowIndex="5" />
      <TextField fx:id="password" GridPane.columnIndex="1" GridPane.rowIndex="6" />
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>

</fx:root>
